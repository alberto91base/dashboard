{
    "name": "reactjs",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@hookform/resolvers": "^2.3.0",
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "axios": "^0.21.1",
        "env-cmd": "^10.1.0",
        "express": "^4.17.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-hook-form": "^7.0.7",
        "react-redux": "^7.2.3",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.9",
        "redux-saga": "^1.1.3",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.2.1",
        "web-vitals": "^1.0.1",
        "yup": "^0.32.9"
    },
    "scripts": {
        "start": "env-cmd -f .env react-app-rewired start",
        "build": "react-app-rewired build",
        "production": "env-cmd -f .env.production node server.js",
        "build:staging": "env-cmd -f .env.staging npm run build",
        "staging": "env-cmd -f .env.staging  node server.js",
        "test": "react-app-rewired test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "cypress": "cypress open",
        "test:e2e": "cypress run"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "plugins": [
            "cypress"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@cypress/react": "^4.16.4",
        "@storybook/addon-actions": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-links": "^6.1.21",
        "@storybook/node-logger": "^6.1.21",
        "@storybook/preset-create-react-app": "^3.1.7",
        "@storybook/react": "^6.1.21",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.1.10",
        "cypress": "^6.8.0",
        "eslint-plugin-cypress": "^2.11.2",
        "react-app-rewire-alias": "^1.0.2",
        "react-app-rewired": "^2.1.8",
        "react-test-renderer": "^17.0.1"
    }
}
